{
  "name": "mapgrid",
  "version": "1.0.0",
  "description": "Extension for show map and grid list on Layout Directus",
  "icon": "extension",
  "author": "Devix Tecnologia <contato@devix.co>",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-layout",
    "map",
    "grid"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "layout",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^10.10.0",
    "sandbox": {
      "enabled": true,
      "requestedScopes": {}
    }
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^13.0.1",
    "typescript": "^5.7.3",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.16",
    "leaflet": "^1.9.4"
  }
}
